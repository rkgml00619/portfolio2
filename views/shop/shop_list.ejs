<%- include('../header', jsFiles=[], cssFiles=[]) %>
    <div id="container" class="shop_list">
        <div class="cont1">
            <div class="center">
                <div class="titleWrap">
                    <h2 class="mainTitle poppins">Clothes</h2>
                    <p class="subTitle poppins">
                        We are Soulmate Fashion, too</p>
                </div>
            </div>
        </div>
        <div class="cont2">
            <div class="center">
                <div class="lists">
                    <% for( let i = 0; i < data.length; i++ ) { %>
                        <div class="list">
                            <a href="/shop/detail/<%- data[i].num %>">
                                <div class="imgWrap">
                                    <img src="/upload/<%- data[i].prdImg[0] %>" alt="">
                                </div>
                                <div class="textWrap">
                                    <h5 class="prdName"><%- data[i].prdName %></h5>
                                    <p class="price"><%- data[i].price %>Ïõê</p>
                                </div>
                            </a>
                        </div>
                    <% } %>
                </div>
            </div>
        </div>
    </div>    
</body>
</html>