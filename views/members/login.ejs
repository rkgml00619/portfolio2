<%- include('../header', jsFiles=[], cssFiles=[]) %>

<div id="container" class="login">
    <div class="center">
        <form id="login_form" action="/loginCheck" method="post">
            <div class="inputWrap">
                <div class="inputs">
                    <label for="memberId">아이디</label>
                    <input type="text" id="memberId" name="memberId" placeholder="아이디를 입력하세요.">
                </div>
                <div class="inputs">
                    <label for="memberPass">비밀번호</label>
                    <input type="password" id="memberPass" name="memberPass" placeholder="비밀번호를 입력하세요.">
                </div>
            </div>
        </form>
    </div>
</div>

</body>
</html>